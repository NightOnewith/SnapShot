<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FocusImageView">
        <attr name="focus_focusing_id" format="integer" />
        <attr name="focus_success_id" format="integer" />
        <attr name="focus_fail_id" format="integer" />
    </declare-styleable>

    <declare-styleable name="TempImageView">
        <attr name="animat_id" format="integer" />
    </declare-styleable>
    <declare-styleable name="verticaltextview">
        <attr name="direction">
            <enum name="uptodown" value="0" />
            <enum name="downtoup" value="1" />
            <enum name="lefttoright" value="2" />
            <enum name="righttoleft" value="3" />
        </attr>
    </declare-styleable>
</resources>
